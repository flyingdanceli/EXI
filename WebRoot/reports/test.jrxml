	<queryString>
		<![CDATA[SELECT
		fi.*,
		sps.`name` sps_name,
		spf.`name` spf_name,
		bcs.`name` bcs_name,
		bcs.phone bcs_phone,
		bcf.`name` bcf_name,
		bcf.phone bcf_phone
	FROM
		fa_invoice fi,
		sys_point sps,
		sys_point spf,
		bas_clientele bcs,
		bas_clientele bcf
	where
		fi.fa_point = spf.id
		AND fi.so_point = sps.id
		AND fi.fa_clientele = bcf.id
		AND fi.so_clientele = bcs.id
 		and  fi.id=$P{invoice_id}]]>
	</queryString>
